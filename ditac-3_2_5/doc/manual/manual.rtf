}
\par
\pard\sb133\li487\ri87\sl266\cbpat21
{\plain\f2\fs18\cf2\chcbpat21
<topicref href="ONE.dita" keys="key_ONE" linking="none" print="no"
}
{\plain\f2\fs18\cf2\chcbpat21
\line
}
{\plain\f2\fs18\cf2\chcbpat21
          toc="no" search="no"/>
}
\par
\pard\sb133\li400\sl266
{\plain\f0\fs20\cf2
Notice default attribute 
}
{\plain\f2\fs18\cf2
